    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: bodyline sports debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/bodyline-sports.csproj",
            "justMyCode": false,
            "suppressJITOptimizations": true,
            "enableStepFiltering": false,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": true, 
                "searchNuGetOrgSymbolServer": true,
                "moduleFilter": {
                    "mode": "loadAllButExcluded",
                    "excludedModules": []
                }
            },
        }

    ]
}